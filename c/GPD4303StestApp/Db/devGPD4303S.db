#
# Short and long form of identification string
#
record(stringin, "$(P):IDN")
{
    field(DESC, "GPD4303S Power Supply")
    field(DTYP, "stream")
    field(INP,  "@devGPD4303S.proto getIDN(39) $(PORT)")
    field(PINI, "YES")
}

record(bo, "$(P):BEEP")
{
    field(DESC, "PD4303S Power Supply BEEP")
    field(DTYP, "stream")
    field(OUT,  "@devGPD4303S.proto setBoolean(BEEP) $(PORT)")
}

record(ai, "$(P):CH$(C):$(VSET_LABEL)")
{
    field(DESC, "Gets output voltage setting")
    field(DTYP, "stream")
    field(SCAN, "1 second")
    field(INP,  "@devGPD4303S.proto getFloat(VSET$(C)) $(PORT)")
    field(MDEL, "-1")
    field(PINI, "YES")
}

record(ai, "$(P):CH$(C):$(VOUT_LABEL)")
{
    field(DESC, "Gets actual output voltage")
    field(DTYP, "stream")
    field(SCAN, "1 second")
    field(INP,  "@devGPD4303S.proto getFloat(VOUT$(C)) $(PORT)")
    field(MDEL, "-1")
    field(PINI, "YES")
}

